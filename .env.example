# LLM provider key used by OpenClaw sessions.
# Set at least one; both are forwarded to each session container.
GOOGLE_API_KEY=your_gemini_api_key_here
GEMINI_API_KEY=

# Optional model override for OpenClaw (default keeps existing Gemini model)
OPENCLAW_MODEL_PRIMARY=google/gemini-1.5-pro-latest

# Agent gateway password exposed in session metadata/UI
OPENCLAW_PASSWORD=1234

# Control-plane settings
HOST=0.0.0.0
PORT=3000
OPENCLAW_IMAGE=boxed-lobster
SESSION_PORT_START=19000
SESSION_PORT_END=19999
SESSION_TTL_MINUTES=120
